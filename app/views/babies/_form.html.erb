<html>
	<head>
		<title>Form</title>
	</head>
	<body>
		<%= form_for @baby do |f| %>
			<%= f.label :first_name %>
			<%= f.text_field :first_name %>
			<%= f.label :last_name %>
			<%= f.text_field :last_name %>
			<%= f.label :weight %>
			<%= f.text_field :weight %>
			<%= f.label :birth_date %>
			<%= f.date_field :birth_date %>
			<%= f.submit %>
		<% end %>
	</body>
</html>
